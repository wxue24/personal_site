  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "parcel": "*",
    "postcss": "^8.3.5",
    "rimraf": "^2.7.1",
    "tailwindcss": "^2.2.4"
  },
  "dependencies": {
    "postcss-import": "^14.0.2"
  }
}
